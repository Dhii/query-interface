{
  "name": "dhii/query-interface",
  "description": "A light query abstraction standard.",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Anton Ukhanev",
      "email": "xedin.unknown@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.1",
    "dhii/collections-interface": "^v0.3.0-alpha1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0 | ^8.0 | ^9.0",
    "vimeo/psalm": "^3.11.7",
    "slevomat/coding-standard": "^6.0",
    "symfony/polyfill-php80": "^1.19"
  },
  "autoload": {
    "psr-4": {
      "Dhii\\Query\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dhii\\Query\\Test\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-initial": "0.1.x-dev"
    }
  }
}
